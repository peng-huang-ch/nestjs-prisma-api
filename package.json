{
  "name": "nestjs-prisma-api",
  "version": "0.0.1",
  "description": "nestjs prisma api template",
  "author": "Peng Huang",
  "keywords": [
    "nestjs",
    "prisma",
    "opentelemetry",
    "alarm"
  ],
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check '**/*.{js,ts,json,yaml,yml,md}'",
    "format:write": "prettier --write '**/*.{js,ts,json,yaml,yml,md}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:migrate:dev": "pnpm dlx prisma migrate dev",
    "prisma:migrate:dev:create": "pnpm dlx prisma migrate dev --create-only",
    "prisma:migrate:deploy": "pnpm dlx prisma migrate deploy",
    "prisma:generate": "pnpm dlx prisma generate",
    "prisma:studio": "pnpm dlx prisma studio",
    "prisma:seed": "pnpm dlx prisma db seed",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/common": "^10.4.11",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.11",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^10.4.11",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.11",
    "@nestjs/swagger": "^8.0.7",
    "@nestjs/terminus": "^10.2.3",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.53.0",
    "@opentelemetry/context-async-hooks": "1.28.0",
    "@opentelemetry/core": "1.28.0",
    "@opentelemetry/exporter-prometheus": "^0.55.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.55.0",
    "@opentelemetry/propagator-b3": "1.28.0",
    "@opentelemetry/propagator-jaeger": "1.28.0",
    "@opentelemetry/resources": "1.28.0",
    "@opentelemetry/sdk-metrics": "1.28.0",
    "@opentelemetry/sdk-node": "^0.55.0",
    "@opentelemetry/sdk-trace-base": "1.28.0",
    "@opentelemetry/semantic-conventions": "1.28.0",
    "@prisma/client": "^5.22.0",
    "@prisma/instrumentation": "^6.0.0",
    "cache-manager": "^5.7.6",
    "cache-manager-ioredis-yet": "^2.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.4.5",
    "google-auth-library": "^9.15.0",
    "ioredis": "^5.3.2",
    "lodash": "^4.17.21",
    "nestjs-otel": "^6.1.1",
    "nestjs-pino": "^4.1.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pino": "^9.5.0",
    "pino-http": "^10.3.0",
    "pino-loki": "^2.3.1",
    "pino-pretty": "^11.3.0",
    "pino-std-serializers": "^7.0.0",
    "prisma-cursorstream": "^0.3.0",
    "prisma-extension-pagination": "^0.7.4",
    "reflect-metadata": "^0.2.2",
    "rotating-file-stream": "^3.2.5",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@faker-js/faker": "^9.2.0",
    "@golevelup/ts-jest": "^0.6.1",
    "@nestjs/cli": "^10.4.8",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.11",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.10.1",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/passport-jwt": "^4.0.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "commitlint": "^19.6.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.7",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.5",
    "prettier": "^3.4.1",
    "prettier-plugin-prisma": "^5.0.0",
    "prisma": "^5.22.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2"
  }
}
